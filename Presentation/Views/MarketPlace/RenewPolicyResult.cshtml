@{
    ViewData["Title"] = "Home Page";
    Layout = "_TestLayout";
}
@model IEnumerable<MerchantAcquirerAPI.Data.Models.Domains.NetworkTab>
<div class="row flex-lg-nowrap">
    <div class="col mb-3">
        <div class="e-panel card">
            <div class="card-body">
                <div class="card-title">
                    <h6 class="mr-2"><span>Policies List</span><small class="px-1"></small></h6>
                </div>
                <div class="e-table">
                    <div class="table-responsive table-lg mt-3">
                        <table class="table table-bordered">
                            <thead>
                                <tr>



                                    <th class="sortable">Tracking Number </th>
                                    <th>Policy Start</th>
                                    <th>Policy End</th>
                                    <th>Policy Type</th>
                                    <th>Created Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>

                                        <td class="text-nowrap align-middle">@item.TrackingNumber</td>
                                        <td class="text-nowrap align-middle"><span>@item.PolicyStartDate</span></td>
                                        <td class="text-nowrap align-middle"><span>@item.PolicyStartEndDate</span></td>
                                        <td class="text-nowrap align-middle"><span>@item.PolicyType</span></td>
                                        <td class="text-nowrap align-middle"><span>@item.CreatedDate</span></td>


                                    </tr>
                                }
                            </tbody>
                        </table>
                        <li class="nav-item"><a class="nav-link px-2" asp-action="Index" asp-controller="MarketPlace"><i class="fa fa-fw fa-th mr-1"></i><span>Home</span></a></li>
                    </div>
                   
                </div>
            </div>
        </div>
    </div>
   
</div>

